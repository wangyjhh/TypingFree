        "@types/jsdom": "^21.1.7",
        "@types/node": "^18.19.64",
        "@unocss/reset": "^0.56.5",
        "@vitejs/plugin-vue": "^4.6.2",
        "@vue/test-utils": "^2.4.6",
        "@vue/tsconfig": "^0.4.0",
        "@wangyjhh/eslint-config": "^1.0.2",
        "eslint": "^9.14.0",
        "jsdom": "^22.1.0",
        "npm-run-all2": "^6.2.6",
        "rollup-plugin-visualizer": "^5.12.0",
        "typescript": "~5.2.2",
